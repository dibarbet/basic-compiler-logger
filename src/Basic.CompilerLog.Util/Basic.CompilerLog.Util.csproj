<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <SignAssembly>true</SignAssembly>
    <PublicSign>true</PublicSign>
    <DelaySign>false</DelaySign>
    <AssemblyOriginatorKeyFile>.\basiccompilerloggerpublic.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis" Version="4.4.0-1.22369.1" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.4.0-1.22369.1" />
    <PackageReference Include="Microsoft.CodeAnalysis.VisualBasic" Version="4.4.0-1.22369.1" />
    <PackageReference Include="MSBuild.StructuredLogger" Version="2.1.669" />
  </ItemGroup>

</Project>
